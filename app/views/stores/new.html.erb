      <div class="container">
        <h1>Add A Store</h1>
      </div>
      <div class="form">
        <%= simple_form_for @store do |f| %>
          <%= f.input :name %>
          <%= f.input :photo, as: :file %>
          <%= f.submit 'Add', class: 'btn btn-ghost-add'%>
        <% end %>
      </div>
</div>
